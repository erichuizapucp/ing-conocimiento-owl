#################################################################
#    Object Properties
#################################################################

###  http://www.pucp.edu.pe/ontologies/unix-family#hasCertification
:hasCertification rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  owl:inverseOf :isCertificationOf ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Unix ;
                  rdfs:range :UnixSpecificationVersionPartition .


###  http://www.pucp.edu.pe/ontologies/unix-family#hasCharacteristicsOf
:hasCharacteristicsOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      owl:inverseOf :inheritsCharacteristicsTo ;
                      rdf:type owl:TransitiveProperty ;
                      rdfs:domain :OS ;
                      rdfs:range :OS .


###  http://www.pucp.edu.pe/ontologies/unix-family#hasLicensingModel
:hasLicensingModel rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   owl:inverseOf :isLicensingModelOf ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :Unix ;
                   rdfs:range :LicensingModelPartition .


###  http://www.pucp.edu.pe/ontologies/unix-family#inheritsCharacteristicsTo
:inheritsCharacteristicsTo rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdf:type owl:TransitiveProperty ;
                           rdfs:domain :OS ;
                           rdfs:range :OS .


###  http://www.pucp.edu.pe/ontologies/unix-family#isCertificationOf
:isCertificationOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :UnixSpecificationVersionPartition ;
                   rdfs:range :Unix .


###  http://www.pucp.edu.pe/ontologies/unix-family#isLicensingModelOf
:isLicensingModelOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain :LicensingModelPartition ;
                    rdfs:range :Unix .


